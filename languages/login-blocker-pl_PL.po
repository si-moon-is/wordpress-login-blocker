# Główne
msgid "Login Blocker"
msgstr "Blokada Logowania"

msgid "Blocks unauthorized login attempts to WordPress"
msgstr "Blokuje nieautoryzowane próby logowania do WordPress"

# Menu admina
msgid "Login Attempts"
msgstr "Próby Logowania"

msgid "Blocked IPs"
msgstr "Zablokowane IP"

msgid "Settings"
msgstr "Ustawienia"

msgid "Statistics & Attempts"
msgstr "Statystyki & Próby"

msgid "Debug & Logs"
msgstr "Debug & Logi"

msgid "Data Export"
msgstr "Eksport Danych"

# Tabele
msgid "IP Address"
msgstr "Adres IP"

msgid "Username"
msgstr "Nazwa użytkownika"

msgid "Date"
msgstr "Data"

msgid "Status"
msgstr "Status"

msgid "Blocked"
msgstr "Zablokowany"

msgid "Failed"
msgstr "Nieudany"

msgid "Country"
msgstr "Kraj"

msgid "Actions"
msgstr "Akcje"

# Przyciski i formularze
msgid "Export CSV"
msgstr "Eksportuj CSV"

msgid "Block IP"
msgstr "Zablokuj IP"

msgid "Enter IP address to block"
msgstr "Wprowadź adres IP do zablokowania"

msgid "Add"
msgstr "Dodaj"

msgid "Unblock"
msgstr "Odblokuj"

msgid "Unblock All"
msgstr "Odblokuj Wszystkie"

msgid "Delete"
msgstr "Usuń"

