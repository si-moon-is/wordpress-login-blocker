msgstr "Email powiadomień"

msgid "Save Settings"
msgstr "Zapisz Ustawienia"

msgid "Debug Mode"
msgstr "Tryb Debugowania"

msgid "Error Notifications"
msgstr "Powiadomienia o Błędach"

# Komunikaty sukcesu
msgid "Settings saved successfully."
msgstr "Ustawienia zapisane pomyślnie."

msgid "IP address blocked successfully."
msgstr "Adres IP został zablokowany."

msgid "IP address unblocked successfully."
msgstr "Adres IP został odblokowany."

msgid "All IPs unblocked successfully."
msgstr "Wszystkie adresy IP zostały odblokowane."

msgid "All records deleted successfully."
msgstr "Wszystkie rekordy zostały usunięte."

# Komunikaty błędów
msgid "Failed login attempt from IP: %s"
msgstr "Nieudana próba logowania z IP: %s"

msgid "IP address %s has been blocked due to too many failed attempts."
msgstr "Adres IP %s został zablokowany z powodu zbyt wielu nieudanych prób."

msgid "Login Blocker Notification"
msgstr "Powiadomienie Blokady Logowania"

msgid "Invalid IP address format."
msgstr "Nieprawidłowy format adresu IP."

msgid "IP address already blocked."
msgstr "Adres IP jest już zablokowany."

msgid "Error occurred while processing your request."
msgstr "Wystąpił błąd podczas przetwarzania żądania."

msgid "No data to export."
msgstr "Brak danych do eksportu."

msgid "Unauthorized"
msgstr "Brak uprawnień"

msgid "Security verification failed."
msgstr "Weryfikacja bezpieczeństwa nie powiodła się."

# Statystyki
msgid "Currently blocked IPs"
msgstr "Obecnie zablokowane IP"

msgid "Recent attempts (1 hour)"
msgstr "Ostatnie próby (1 godzina)"

msgid "All attempts"
msgstr "Wszystkie próby"

msgid "Unique IPs"
msgstr "Unikalne IP"

msgid "Attack countries"
msgstr "Kraje ataków"

